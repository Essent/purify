{
  "name": "pure-ts",
  "version": "0.0.1",
  "description": "Functional programming standard library for TypeScript ",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/gigobyte/pure.git",
  "author": "gigobyte <gigobest2@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "docs": "typedoc --out documentation/ src/ --theme minimal",
    "test": "jest",
    "start": "parcel site/index.html"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "src/.+\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverage": true
  },
  "devDependencies": {
    "@types/jest": "^22.1.1",
    "@types/react": "^16.1.0",
    "@types/react-dom": "^16.0.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "jest": "^22.2.1",
    "parcel-bundler": "^1.7.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "styled-components": "^3.2.5",
    "ts-jest": "^22.0.3",
    "ts-node": "^4.1.0",
    "typedoc": "^0.11.1",
    "typescript": "2.8.0-rc"
  },
  "dependencies": {}
}
