{
  "name": "pure-ts",
  "version": "0.0.1",
  "description": "Functional programming standard library for TypeScript ",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/gigobyte/pure.git",
  "author": "gigobyte <gigobest2@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "docs": "typedoc --out documentation/ src/ --theme minimal",
    "test": "jest"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "src/.+\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverage": true
  },
  "devDependencies": {
    "@types/jest": "^22.1.1",
    "jest": "^22.2.1",
    "ts-jest": "^22.0.3",
    "ts-node": "^4.1.0",
    "typescript": "2.8.0-rc"
  },
  "dependencies": {}
}
